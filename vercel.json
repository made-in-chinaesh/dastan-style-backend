{
	"version": 2,
	"env": {
		"PORT": "5000",
		"POSTGRES_HOST": "localhost",
		"POSTGRES_USERNAME": "postgres",
		"POSTGRES_DATABASE": "chat",
		"POSTGRES_PASSWORD": "bekaroot",
		"POSTGRES_PORT": "5432",
		"ACCESS_TOKEN_SECRET": "access-secret",
		"REFRESH_TOKEN_SECRET": "refresh-secret"
	},
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": ["GET", "POST", "PUT", "DELETE"]
		}
	]
}
